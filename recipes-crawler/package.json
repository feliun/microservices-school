{
  "name": "recipes-crawler",
  "version": "1.0.3",
  "description": "A crawler to pull recipes into our system",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "server": "supervisor .",
    "test": "mocha test",
    "lint": "eslint .",
    "qa": "npm run lint && npm run test",
    "docker": "bash -c '(docker network inspect local 2>&1 > /dev/null || docker network create local) && docker-compose --file docker-compose.yml pull && docker-compose --file docker-compose.yml up -d --force-recreate'",
    "precommit": "npm run lint",
    "prepush": "npm run test"
  },
  "keywords": [
    "microservice",
    "systemic",
    "confabulous",
    "prepper",
    "example"
  ],
  "author": "GuideSmiths Ltd",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^3.0.1",
    "eslint-config-imperative-es6": "^1.0.0",
    "eslint-plugin-mocha": "^4.0.0",
    "expect.js": "^0.3.1",
    "husky": "^0.11.4",
    "mocha": "^2.5.3",
    "supertest": "^3.0.0",
    "supertest-as-promised": "^4.0.2"
  },
  "dependencies": {
    "R": "0.0.1",
    "aws-sdk": "^2.94.0",
    "body-parser": "^1.15.2",
    "boom": "^3.2.2",
    "bunyan": "^1.8.5",
    "chalk": "^1.1.3",
    "confabulous": "^1.1.0",
    "debug": "^2.6.8",
    "hogan.js": "^3.0.2",
    "http-proxy-middleware": "^0.17.4",
    "logs-to-sumologic": "^1.1.0",
    "make-manifest": "^1.0.1",
    "nock": "^9.0.13",
    "node-ssh": "^4.2.3",
    "on-headers": "^1.0.1",
    "optimist": "^0.6.1",
    "optional": "^0.1.3",
    "pify": "^3.0.0",
    "prepper": "^1.1.0",
    "ramda": "^0.21.0",
    "superagent": "^3.5.2",
    "systemic": "^1.3.0",
    "systemic-domain-runner": "^1.1.0",
    "systemic-express": "^1.0.1",
    "systemic-pg": "^1.0.6",
    "systemic-rabbitmq": "^1.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guidesmiths/svc-example.git"
  },
  "bugs": {
    "url": "https://github.com/guidesmiths/svc-example/issues"
  },
  "homepage": "https://github.com/guidesmiths/svc-example#readme"
}
